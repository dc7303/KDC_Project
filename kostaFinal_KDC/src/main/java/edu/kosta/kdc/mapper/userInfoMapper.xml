<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="userMapper">
    
    <insert id="userInsert" parameterType="userInfoDTO">
        insert into userInfo values(#{userId}, #{userPwd}, #{userName}, #{auth})
    </insert>
    
    <select id="findByUserId" parameterType="String" resultType="userInfoDTO">
        select * from userInfo where userId = #{_parameter}
    </select>
</mapper>